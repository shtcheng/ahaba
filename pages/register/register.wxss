/**account.wxss**/
.register{
  width:100%;
}
.register_phone{
  display: flex;
  margin:20px;
  margin-left: 10px;
  background:#F0F0F0;
}
.register_phone_lable{
  margin:7px;
  width:20%;
  font-size:11pt;
}
.register_phone_input{
  width:80%;
  margin:5px;
  font-size:11pt;
}

.register_code{
  width: 92%;
  display: flex;
  margin-left: 10px;
  background:#F0F0F0;
}
.register_code_lable{
  margin:7px;
  width:20%;
  font-size:11pt;
}
.register_code_input{
  margin:5px;
  width: 57%;
  font-size:11pt;
}
.register_code_butt{
  width:23%;
  font-size:11pt; 
  border:none; 
  border-radius: 0;
}


.register_reg{  
  margin-left: 10px;
  margin:10px;
  width: 92%;
}
.register_reg_butt{  
  font-size:12pt;
  width:100%;
  border:none;
  border-radius: 0;
  margin-top:40px;
}
.register_reg_checkbox{  
  width:100%;
  margin:10px;
}
